<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="350dp"
    android:layout_height="350dp"
    android:layout_gravity="center">

    <ImageView
        android:id="@+id/iv_bg"
        android:layout_width="350dp"
        android:layout_height="350dp"
        android:src="@drawable/fab_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/iv_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/menu_back_s"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/iv_stop_pro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/menu_stop_pro_s"
        android:gravity="center"
        android:text="@string/stop_pro"
        android:textColor="@drawable/black_white_text_pressed"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.93"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.63" />

    <TextView
        android:id="@+id/iv_screenshot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/menu_screenshot_s"
        android:gravity="center"
        android:text="@string/Screenshot_annotation"
        android:textColor="@drawable/black_white_text_pressed"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.765"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.88" />

    <TextView
        android:id="@+id/iv_stop_screen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/menu_stop_screen_s"
        android:gravity="center"
        android:text="@string/stop_screen"
        android:textColor="@drawable/black_white_text_pressed"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.95" />

    <TextView
        android:id="@+id/iv_join_screen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/menu_join_screen_s"
        android:gravity="center"
        android:text="@string/join_screen"
        android:textColor="@drawable/black_white_text_pressed"
        app:layout_constraintBottom_toBottomOf="@+id/iv_screenshot"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.235"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv_screenshot" />

    <TextView
        android:id="@+id/iv_launch_screen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/menu_start_screen_s"
        android:gravity="center"
        android:text="@string/launch_screen"
        android:textColor="@drawable/black_white_text_pressed"
        app:layout_constraintBottom_toBottomOf="@+id/iv_stop_pro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv_stop_pro" />

    <TextView
        android:id="@+id/iv_call_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/menu_call_service_s"
        android:gravity="center"
        android:text="@string/call_service"
        android:textColor="@drawable/black_white_text_pressed"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.93"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.35" />

    <TextView
        android:id="@+id/iv_launch_pro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/menu_start_pro_s"
        android:gravity="center"
        android:text="@string/launch_pro"
        android:textColor="@drawable/black_white_text_pressed"
        app:layout_constraintBottom_toBottomOf="@+id/iv_call_service"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv_call_service" />

    <TextView
        android:id="@+id/iv_meet_note"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/menu_meet_note_s"
        android:gravity="center"
        android:text="@string/meeting_note"
        android:textColor="@drawable/black_white_text_pressed"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/iv_screenshot"
        app:layout_constraintStart_toStartOf="@+id/iv_screenshot"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.12" />

    <TextView
        android:id="@+id/iv_usb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/menu_usb_s"
        android:gravity="center"
        android:text="@string/temporary_materials"
        android:textColor="@drawable/black_white_text_pressed"
        app:layout_constraintBottom_toBottomOf="@+id/iv_meet_note"
        app:layout_constraintEnd_toEndOf="@+id/iv_join_screen"
        app:layout_constraintStart_toStartOf="@+id/iv_join_screen"
        app:layout_constraintTop_toTopOf="@+id/iv_meet_note"
        app:layout_constraintVertical_bias="0.10" />

    <TextView
        android:id="@+id/iv_meet_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableTop="@drawable/menu_meet_info_s"
        android:gravity="center"
        android:text="@string/meeting_information"
        android:textColor="@drawable/black_white_text_pressed"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.05" />
</androidx.constraintlayout.widget.ConstraintLayout>